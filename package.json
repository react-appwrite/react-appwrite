{
  "name": "react-appwrite",
  "description": "React hooks for Appwrite.",
  "author": "sanny-io",
  "version": "0.0.5",
  "license": "MIT",
  "repository": "github:sanny-io/react-appwrite",
  "homepage": "https://github.com/sanny-io/react-appwrite",
  "bugs": "https://github.com/sanny-io/react-appwrite/issues",
  "keywords": [
    "react",
    "appwrite",
    "hooks",
    "firebase"
  ],
  "scripts": {
    "dev": "rimraf dist && tsc -w --preserveWatchOutput",
    "build": "rimraf dist && tsc",
    "lint": "npm run build && eslint --ext .ts,.tsx ./",
    "prepare": "npm run build && husky install",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules DOTENV_CONFIG_PATH=.env.test jest --setupFiles=dotenv/config --rootDir tests --testRegex .*",
    "prepublishOnly": "npm run lint"
  },
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./account": "./dist/account/index.js",
    "./databases": "./dist/databases/index.js",
    "./functions": "./dist/functions/index.js",
    "./locale": "./dist/locale/index.js",
    "./storage": "./dist/storage/index.js",
    "./avatars": "./dist/avatars/index.js",
    "./next": "./dist/next/index.js",
    "./teams": "./dist/teams/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "account.d.ts",
    "databases.d.ts",
    "functions.d.ts",
    "locale.d.ts",
    "storage.d.ts",
    "avatars.d.ts",
    "next.d.ts",
    "teams.d.ts"
  ],
  "engines": {
    "node": ">=10"
  },
  "peerDependencies": {
    "appwrite": "^10.2.0",
    "react": ">=16"
  },
  "module": "dist/index.js",
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "appwrite": "^10.2.0",
    "babel-jest": "^29.5.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.37.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "miragejs": "^0.1.47",
    "next": "^13.1.6",
    "react": ">=16",
    "react-dom": "^18.2.0",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "optionalDependencies": {
    "encoding": "^0.1.13"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.24.9",
    "@tanstack/react-query-devtools": "^4.24.9",
    "immer": "^9.0.19"
  }
}